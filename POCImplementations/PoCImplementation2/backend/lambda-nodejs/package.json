{
  "name": "learningyogi-lambda-nodejs",
  "version": "1.0.0",
  "description": "Learning Yogi - Node.js Lambda Functions for Serverless PoC",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
