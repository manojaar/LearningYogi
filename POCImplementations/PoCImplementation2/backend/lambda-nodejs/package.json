    "@aws-sdk/lib-storage": "^3.470.0",
    "aws-lambda": "^1.0.7",
    "aws-xray-sdk-core": "^3.5.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
